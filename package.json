{
	"name": "slither",
	"version": "1.2.2",
	"license": "MIT",
	"scripts": {
		"lint": "yarn eslint src && yarn prettier --check src",
		"compile": "rbxtsc --verbose",
		"watch": "rbxtsc -w",
		"tarmac": "tarmac sync --target roblox",
		"serve": "rojo serve",
		"build:dev": "rm -rf out && yarn compile && rojo build --output place.rbxlx",
		"build:prod": "rm -rf out && cross-env NODE_ENV=production yarn compile && rojo build --output place.rbxlx",
		"deploy:dev": "yarn build:dev && mantle deploy --environment canary",
		"deploy:prod": "yarn build:prod && mantle deploy --environment production",
		"output:dev": "mantle outputs --environment canary > mantle-output.json",
		"output:prod": "mantle outputs --environment production > mantle-output.json"
	},
	"devDependencies": {
		"@rbxts/compiler-types": "^2.2.0-types.0",
		"@rbxts/types": "^1.0.718",
		"@typescript-eslint/eslint-plugin": "^6.7.3",
		"@typescript-eslint/parser": "^6.7.4",
		"cross-env": "^7.0.3",
		"eslint": "^8.50.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-roblox-ts": "^0.0.36",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"eslint-plugin-unused-imports": "^3.0.0",
		"prettier": "^3.0.3",
		"rbxts-transform-debug": "^2.2.0",
		"rbxts-transform-env": "^2.2.0",
		"roblox-ts": "^2.2.0",
		"typescript": "^5.2.2"
	},
	"dependencies": {
		"@rbxts/bitbuffer2": "^1.0.0-ts.0",
		"@rbxts/flipper": "^2.0.1",
		"@rbxts/lapis": "^0.2.4",
		"@rbxts/object-utils": "^1.0.4",
		"@rbxts/pretty-react-hooks": "^0.3.2",
		"@rbxts/react-reflex": "^0.2.0",
		"@rbxts/react-roblox": "^0.2.0",
		"@rbxts/reflex": "^4.2.2",
		"@rbxts/remo": "^1.2.3",
		"@rbxts/ripple": "^0.7.0",
		"@rbxts/roact": "npm:@rbxts/react-ts@latest",
		"@rbxts/services": "^1.5.1",
		"@rbxts/set-timeout": "^1.1.2",
		"@rbxts/t": "^3.1.0",
		"@rbxts/testez": "^0.4.2-ts.0",
		"@rbxts/validate-tree": "^2.0.2"
	}
}
